   ],
   "source": [
    "import threading\n",
    "\n",
    "def print_numbers():\n",
    "    for i in range(5):\n",
    "        print(f\"Number: {i}\")\n",
    "\n",
    "t1 = threading.Thread(target=print_numbers)\n",
    "t1.start()\n",
    "t1.join()\n"
   ]
